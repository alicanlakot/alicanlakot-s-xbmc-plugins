<scraperfunctions>
	<GetTMDBTrailerByIdChain dest="4">
		<RegExp input="$$5" output="&lt;details&gt;\1&lt;/details&gt;" dest="4">
			<RegExp input="$$1" output="&lt;url function=&quot;ParseTMDBTrailer&quot; cache=&quot;tmdb-$INFO[language]-\1.xml&quot;&gt;http://api.themoviedb.org/2.1/Movie.getInfo/$INFO[language]/xml/57983e31fb435df4df77afb854740ea9/\1&lt;/url&gt;" dest="5">
				<expression>^([0-9]+)$</expression>
			</RegExp>
			<expression noclean="1"/>
		</RegExp>
	</GetTMDBTrailerByIdChain>
	<ParseTMDBTrailer dest="5">
		<RegExp input="$$2" output="&lt;details&gt;\1&lt;/details&gt;" dest="5">
			<RegExp input="$$1" output="&lt;trailer&gt;plugin://plugin.video.youtube/?action=play_video&amp;videoid=\1&lt;/trailer&gt;" dest="2">
				<expression noclean="1">&lt;trailer&gt;.*?youtube.com/watch\?v=([^&lt;]*)&lt;/trailer&gt;</expression>
			</RegExp>
			<expression noclean="1">(.+)</expression>
		</RegExp>
	</ParseTMDBTrailer>
